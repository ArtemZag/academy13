@model BinaryStudio.PhotoGallery.Web.ViewModels.InfoViewModel
@{
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<div id="photoWrapper">
    @foreach (var photo in Model.Photos)
    {
        <div class="photoContainer">
            <img src="@photo.PhotoSource"/>
        </div>
    }								
</div>