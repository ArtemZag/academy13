@{
    ViewBag.Title = ;
}

<!DOCTYPE html>
<html>
    <head>
        <title>Page Not Found | Bingally</title>
        @Styles.Render("~/Content/notfound-page.css")
    </head>

    <body>
        <div id="container">
            <div id="image"><img src="~/Content/"/></div>

        </div>

        <p>Could not find the page: @ViewBag.RequestedUrl</p>
        @if (ViewBag.ReferrerUrl != null) {
            <p>Go back to the <a href="@ViewBag.ReferrerUrl">previous page</a>.</p>
        }
    </body>
</html>